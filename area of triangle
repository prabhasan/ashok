#include <stdio.h>
#include <stdlib.h>
float area(int, int);
int main()
{
float area(base, height)
}
{
area(base, height);
printf("enter the base of a triangle:");
scanf("%d", &base);
printf("enter the height of a triangle:");
scanf("%d", &height);
area=(base*height)/2.0;
printf("area of a triangle=%f",area);
return 0;
}
